{\n            [name]: value\n        });\n    };\n\n    handleFormSubmit = event => {\n        event.preventDefault();\n        this.searchBook(this.state.search);\n    };\n\n    render() {\n        return (\n          <div>\n            <h1 className=\"text-center\">Search Books!</h1>\n            <p className=\"text-center\">Use the search bar to look for various book titles.  Click view to find out more about the book.  Click save to add that book to your list of unread books.</p>\n            <Form\n              handleFormSubmit={this.handleFormSubmit}\n              handleInputChange={this.handleInputChange}\n              search={this.state.search}\n            />\n            <Results\n              books={this.state.books}\n            ></Results>\n          </div>\n        );\n      }\n}\n\nexport default Search;",{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":3,"column":8,"nodeType":"55","messageId":"56","endLine":3,"endColumn":15},{"ruleId":"53","severity":1,"message":"57","line":2,"column":7,"nodeType":"55","messageId":"56","endLine":2,"endColumn":14},{"ruleId":"53","severity":1,"message":"58","line":3,"column":7,"nodeType":"55","messageId":"56","endLine":3,"endColumn":14},{"ruleId":"59","severity":1,"message":"60","line":5,"column":1,"nodeType":"61","endLine":21,"endColumn":5},{"ruleId":"53","severity":1,"message":"62","line":1,"column":17,"nodeType":"55","messageId":"56","endLine":1,"endColumn":26},"no-native-reassign",["63"],"no-negated-in-lhs",["64"],"no-unused-vars","'styling' is defined but never used.","Identifier","unusedVar","'API_URL' is assigned a value but never used.","'API_KEY' is assigned a value but never used.","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","'Component' is defined but never used.","no-global-assign","no-unsafe-negation"]